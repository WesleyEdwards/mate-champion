    "firebase": "^9.18.0",
    "lodash": "^4.17.21",
    "material-icons": "^1.13.6",
    "prettier": "^3.3.3",
    "react": "^18.2.0",
    "react-colorful": "^5.6.1",
    "react-dom": "^18.2.0",
    "vite-plugin-checker": "^0.5.6"
  },
  "devDependencies": {
    "@types/lodash": "^4.17.5",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@vitejs/plugin-react": "^3.1.0",
    "typescript": "^5.6.2",
    "vite": "^4.2.0"
